{
  "name": "ACSU Easy Wi-Fi Connect",
  "description": "信州大学学内ネットワークにブラウザから容易にログインを可能にするChrome拡張",
  "version": "1.0",
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "js/jquery-3.3.1.js", "js/content.js" ]
    }
  ],
  "permissions": [ "contextMenus", "<all_urls>", "contextMenus", "activeTab" ],
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "manifest_version": 2
}